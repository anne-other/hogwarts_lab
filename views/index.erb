<h1>All Students</h1>


<% @students.each do |student| %>
  <p>Name: <%= student.first_name %> <%= student.second_name %></p>
  <p>House: <%= student.find_house_for_student.name %></p>
  <p>Age: <%= student.age %></p>
  <hr>
  <% end %>

<form action="students/new-student" method="get">
  <input type="submit" value="Add Student">
</form>
